{
    "id": "com.liuyang.psbanana",
    "name": "PS Banana",
    "version": "2.3.0",
    "main": "index.html",
    "host": {
        "app": "PS",
        "minVersion": "23.0.0",
        "apiVersion": 2
    },
    "manifestVersion": 5,
    "requiredPermissions": {
        "network": {
            "domains": [
                "https://generativelanguage.googleapis.com",
                "https://yunwu.zeabur.app",
                "https://yunwu.ai",
                "https://googlecdn.datas.systems",
                "https://*.datas.systems",
                "https://api.gptgod.online",
                "https://*.gptgod.online",
                "https://openrouter.ai",
                "https://*.openrouter.ai",
                "https://ark.cn-beijing.volces.com",
                "https://*.volces.com",
                "https://*.volccdn.com",
                "https://*.googleapis.com",
                "https://*.googleusercontent.com",
                "https://filesystem.site",
                "https://*.filesystem.site",
                "https://storage.googleapis.com",
                "https://*.storage.googleapis.com",
                "https://*.cloudfront.net",
                "https://*.amazonaws.com",
                "https://*.blob.core.windows.net",
                "https://*.oaistatic.com",
                "https://*.openaiusercontent.com",
                "https://*.vercel-storage.com",
                "https://*.r2.cloudflarestorage.com",
                "https://*.imgix.net",
                "https://*.imgur.com",
                "https://*.discordapp.com",
                "https://cdn.discordapp.com",
                "https://*.alicdn.com",
                "https://*.aliyuncs.com",
                "https://*.qiniu.com",
                "https://*.qiniucdn.com",
                "https://*.byteimg.com",
                "https://*.b-cdn.net",
                "http://127.0.0.1",
                "http://127.0.0.1:3000",
                "http://127.0.0.1:5000",
                "http://127.0.0.1:8000",
                "http://127.0.0.1:8045",
                "http://127.0.0.1:8045/",
                "http://127.0.0.1:8080",
                "http://127.0.0.1:8188",
                "http://localhost",
                "http://localhost:3000",
                "http://localhost:5000",
                "http://localhost:8000",
                "http://localhost:8045",
                "http://localhost:8045/",
                "http://localhost:8080",
                "http://localhost:8188"
            ]
        },
        "localFileSystem": "fullAccess"
    },
    "icons": [
        {
            "width": 48,
            "height": 48,
            "path": "icons/plugin.png",
            "scale": [
                1,
                2
            ],
            "theme": [
                "darkest",
                "dark",
                "medium",
                "lightest",
                "light",
                "all"
            ],
            "species": [
                "pluginList"
            ]
        }
    ],
    "entrypoints": [
        {
            "type": "command",
            "id": "reloadPlugin",
            "label": "Reload Plugin"
        },
        {
            "type": "panel",
            "id": "psbanana",
            "icons": [
                {
                    "width": 23,
                    "height": 23,
                    "path": "icons/dark.png",
                    "scale": [
                        1,
                        2
                    ],
                    "theme": [
                        "darkest",
                        "dark",
                        "medium"
                    ]
                },
                {
                    "width": 23,
                    "height": 23,
                    "path": "icons/light.png",
                    "scale": [
                        1,
                        2
                    ],
                    "theme": [
                        "lightest",
                        "light"
                    ]
                }
            ],
            "minimumSize": {
                "width": 320,
                "height": 400
            },
            "maximumSize": {
                "width": 600,
                "height": 2000
            },
            "preferredDockedSize": {
                "width": 320,
                "height": 600
            },
            "preferredFloatingSize": {
                "width": 320,
                "height": 600
            },
            "label": {
                "default": "PS Banana"
            }
        }
    ]
}
